#-------------------------------------------------------------------------------
# gnuplotの設定
#-------------------------------------------------------------------------------
reset
set nokey                # 凡例の非表示
set xrange[0.0005:0.0045]
set yrange[0.0015:0.0045]
set size square

set parametric

set term gif enhanced animate size 2048,2048     # 出力をgifアニメに設定
set output "karman_omp_scale.gif" # 出力ファイル名の設定
#-------------------------------------------------------------------------------
# 変数の設定
#-------------------------------------------------------------------------------
n0 = 0    # ループ変数の初期値
n1 = 147   # ループ変数の最大値
dn = 1    # ループ変数の増加間隔
adj = 0.005	#ベクトルの長さ

#-------------------------------------------------------------------------------
# ループの開始
#-------------------------------------------------------------------------------
load "karman_scale.plt" 
