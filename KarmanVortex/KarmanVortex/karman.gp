#-------------------------------------------------------------------------------
# gnuplotの設定
#-------------------------------------------------------------------------------
reset
set nokey                # 凡例の非表示
set xrange[0:0.2]
set yrange[0:0.2]
set size square

set parametric
#set term png size 1080,1080

set term gif enhanced animate size 1080,1080     # 出力をgifアニメに設定
set output "karman.gif" # 出力ファイル名の設定
#-------------------------------------------------------------------------------
# 変数の設定
#-------------------------------------------------------------------------------
n0 = 0    # ループ変数の初期値
n1 = 40   # ループ変数の最大値
dn = 1    # ループ変数の増加間隔
adj = 0.1	#ベクトルの長さ

#-------------------------------------------------------------------------------
# ループの開始
#-------------------------------------------------------------------------------
load "karman.plt" 
